<% include partials/header %>
   <div id="banner-area" class="banner-area" style="background-image:url(../constra/images/banner/banner2.jpg)">
      <div class="banner-text">
         <div class="container">
            <div class="row">
               <div class="col-xs-12">
                  <div class="banner-heading">
                     <h1 class="banner-title">Projects</h1>
                     <ol class="breadcrumb">
                        <li>Home</li>
                        <li>Projects</li>
                        <li><a href="#">Projects Single</a></li>
                     </ol>
                  </div>
               </div><!-- Col end -->
            </div><!-- Row end -->
         </div><!-- Container end -->
      </div><!-- Banner text end -->
   </div><!-- Banner area end --> 
  <section id="main-container" class="main-container" style="overflow: hidden;">
      <div class="container" style="border:2px solid red;">
         <div class="row" style="overflow: hidden; border:2px solid blue;">
            <div class="col-md-8">
               <div id="page-slider" class="owl-carousel owl-theme page-slider page-slider-small">
                  <div class="item">
                     <img src="../constra/images/projects/project5.jpg" alt="" />
                  </div>
                  <div class="item">
                     <img src="../constra/images/projects/project4.jpg" alt="" />
                  </div>
               </div><!-- Page slider end -->
            </div><!-- Slider col end -->
            <div class="col-md-4" style="border:2px solid black;">
               <h3 class="column-title mrt-0"><%= title %></h3>
               <p><%= para1 %></p>
               <p><%= para2 %></p>
               <p><%= para3 %></p>
               <p><i><b><%= quotes %></b></i></p>
            </div><!-- Content col end -->
         </div><!-- Row end -->
      </div><!-- Conatiner end -->
   </section><!-- Main container end -->
<% include partials/footer %>